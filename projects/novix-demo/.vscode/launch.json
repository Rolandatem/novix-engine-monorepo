{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Novix Demo in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*",
        "webpack:///libs/*": "${workspaceFolder}/../../libs/*"
      }
    }
  ]
}
